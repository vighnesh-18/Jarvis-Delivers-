# AI Service Environment Variables Template
# Copy this file to .env and fill in your actual values

# AI Service Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Disable OpenAI completely - remove or comment out if you want to use OpenAI
# OPENAI_API_KEY=your_openai_api_key_here

# Disable CrewAI telemetry
OTEL_SDK_DISABLED=true
CREWAI_TELEMETRY_DISABLED=true

# Force LiteLLM to use Gemini
LITELLM_MODEL=gemini/gemini-2.5-flash

# Database
MONGODB_URI=mongodb://localhost:27017/jarvis-delivers

# Service URLs
AI_SERVICE_PORT=8000
NODE_BACKEND_URL=http://localhost:5002
